{
  "name": "ananta-angular",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build",
    "build:android": "ng build",
    "lint": "ng lint",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:lite": "node -e \"const {spawnSync}=require('node:child_process'); const env={...process.env,ANANTA_E2E_USE_EXISTING:'1',E2E_REUSE_SERVER:'1',E2E_REPORTER_MODE:'compact'}; const r=spawnSync('npx',['playwright','test'],{stdio:'inherit',shell:true,env}); process.exit(r.status ?? 1);\"",
    "test:e2e:live": "node -e \"const {spawnSync}=require('node:child_process'); const env={...process.env,RUN_LIVE_LLM_TESTS:'1'}; const r=spawnSync('npx',['playwright','test','tests/templates-ai-live.spec.ts'],{stdio:'inherit',shell:true,env}); process.exit(r.status ?? 1);\"",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "cap:sync": "npx cap sync android",
    "cap:copy": "npx cap copy android",
    "cap:open:android": "npx cap open android",
    "android:prepare": "npm run build:android && npm run cap:sync"
  },
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@capacitor/android": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "dompurify": "^3.1.6",
    "marked": "^14.0.0",
    "mermaid": "^11.12.2",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular-eslint/builder": "^21.0.0",
    "@angular-eslint/eslint-plugin": "^21.0.0",
    "@angular-eslint/eslint-plugin-template": "^21.0.0",
    "@angular-eslint/schematics": "^21.0.0",
    "@angular-eslint/template-parser": "^21.0.0",
    "@angular/cdk": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@axe-core/playwright": "^4.11.1",
    "@capacitor/cli": "^7.4.2",
    "@playwright/test": "^1.48.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "jsdom": "^28.1.0",
    "otplib": "^13.2.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "zone.js": "^0.16.0"
  },
  "overrides": {
    "lodash-es": "4.17.23"
  }
}
